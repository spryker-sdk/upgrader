{
    "name": "spryker-sdk/upgrader",
    "type": "library",
    "description": "Code evaluator and upgrader tool",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-simplexml": "*",
        "composer/semver": "^3.3",
        "davidrjonas/composer-lock-diff": "^1.6",
        "ergebnis/json-printer": "^3.1",
        "guzzlehttp/guzzle": "^7.4",
        "http-interop/http-factory-guzzle": "^1.0",
        "knplabs/github-api": "^3.4",
        "m4tthumphrey/php-gitlab-api": "^11.5",
        "nikic/php-parser": "4.15.*",
        "phpstan/phpstan": "^1.2",
        "spryker-sdk/azure-php-client": "dev-master as 1.0.0",
        "spryker-sdk/integrator": "dev-master as 1.0.0",
        "spryker-sdk/sdk-contracts": "^0.4.7",
        "symfony/config": "^5.0",
        "symfony/console": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/filesystem": "^5.3",
        "symfony/finder": "^5.3",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-kernel": "^5.0",
        "symfony/monolog-bundle": "^3.8",
        "symfony/process": "^5.0",
        "symfony/runtime": "^5",
        "symfony/serializer": "^5.4",
        "symfony/uid": "^5.4",
        "symfony/yaml": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "spryker/code-sniffer": "dev-master",
        "mikey179/vfsstream": "^1.6.7",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "src/Core/",
            "Codebase\\": "src/Codebase/",
            "Evaluate\\": "src/Evaluate/",
            "Upgrade\\": "src/Upgrade/",
            "Upgrader\\": "src/Upgrader/",
            "ReleaseApp\\": "src/ReleaseApp/",
            "DynamicEvaluator\\": "src/DynamicEvaluator/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodebaseTest\\": "tests/CodebaseTest/",
            "CoreTest\\": "tests/Core/",
            "TestProject\\": "tests/data/Evaluate/Project/",
            "TestCore\\": "tests/data/Evaluate/Core/",
            "UpgradeData\\": "tests/data/Upgrade/",
            "UpgraderTest\\": "tests/UpgraderTest/",
            "DynamicEvaluatorTest\\": "tests/DynamicEvaluatorTest/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "symfony/runtime": true,
            "php-http/discovery": false
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "stan": "phpstan analyze src/",
        "linters": [
            "@cs-fix",
            "@cs-check",
            "@stan"
        ],
        "tests": "phpunit tests/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
